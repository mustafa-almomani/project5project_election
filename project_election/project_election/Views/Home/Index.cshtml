        <div id="adCarousel" class="carousel slide container mt-5" data-bs-ride="carousel">
            <div class="carousel-inner">
                @foreach (var chunk in Model.Select((value, index) => new { value, index }).GroupBy(x => x.index / 3))
                {
                    <div class="carousel-item @(chunk.Key == 0 ? "active" : "")">
                        <div class="row">
                            @foreach (var ad in chunk)
                            {
                                <div class="col-md-4">
                                    <div class="card mx-auto" style="width: 25rem;">
                                        @{
                                            var url = ad.value.URLPath?.ToLower();
                                            if (url != null)
                                            {
                                                if (url.Contains("youtube.com") || url.Contains("youtu.be"))
                                                {
                                                    string embedUrl = "";
                                                    if (url.Contains("youtube.com"))
                                                    {
                                                        var videoId = System.Text.RegularExpressions.Regex.Match(url, @"(?:youtube\.com/(?:[^/]+/.+/|(?:v|e(?:mbed)?)|.*[?&]v=)|youtu\.be/)([^""&?/ ]{11})").Groups[1].Value;
                                                        embedUrl = $"https://www.youtube.com/embed/{videoId}";
                                                    }
                                                    else if (url.Contains("youtu.be"))
                                                    {
                                                        var videoId = url.Split('/').Last();
                                                        embedUrl = $"https://www.youtube.com/embed/{videoId}";
                                                    }
                                                    <iframe width="100%" height="400" src="@embedUrl" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>
                                                }
                                                else if (url.Contains("vimeo.com"))
                                                {
                                                    var videoId = url.Split('/').Last();
                                                    var embedUrl = $"https://player.vimeo.com/video/{videoId}";
                                                    <iframe width="100%" height="400" src="@embedUrl" title="Vimeo video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>
                                                }
                                                else if (url.Contains(".mp4") || url.Contains(".webm") || url.Contains(".ogg"))
                                                {
                                                    <video controls class="img-fluid">
                                                        <source src="@ad.value.URLPath" type="video/mp4">
                                                        Your browser does not support the video tag.
                                                    </video>
                                                }
                                                else if (url.EndsWith(".jpg") || url.EndsWith(".jpeg") || url.EndsWith(".png") || url.EndsWith(".gif"))
                                                {
                                                    <img src="@ad.value.URLPath" class="card-img-top" alt="advertisement image">
                                                }
                                                else
                                                {
                                                    <img src="@ad.value.URLPath" class="card-img-top" alt="advertisement image">
                                                }
                                            }
                                            else
                                            {
                                                <p>No URL provided.</p>
                                            }
                                        }

                                        <div class="card-body">
                                            <p class="card-text"> <b> اسم المرشح :@ad.value.CandidateName </b></p>
                                            <p class="card-text"> اسم القائمة : @ad.value.ListName </p>
                                            <p class="card-text">@ad.value.Content</p>

                                        </div>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                }
                <button class="carousel-control-prev" type="button" data-bs-target="#adCarousel" data-bs-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#adCarousel" data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                </button>
            </div>
    </main>
